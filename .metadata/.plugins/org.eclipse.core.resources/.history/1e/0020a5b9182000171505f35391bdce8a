import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.io.OutputStreamWriter;
import java.io.PrintWriter;
import java.util.Arrays;
import java.util.HashSet;
import java.util.Iterator;


public class SubArrayWithGivenSum {
	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		PrintWriter pw = new PrintWriter(new OutputStreamWriter(System.out));

		int t = Integer.parseInt(br.readLine().trim());
		while (t-- > 0) {
			//String str = br.readLine().trim();
			String sArr[]=br.readLine().trim().split(" ");
			int n = Integer.parseInt(sArr[0].trim());
			int x = Integer.parseInt(sArr[1].trim());
			sArr=br.readLine().trim().split(" ");
			int arr[]=new int[n];
			int carr[]=new int[n+1];
			int  val=0,i=0;
			carr[0]=0;
			for(String s:sArr){
				arr[i]=Integer.parseInt(s.trim());
				val=val+arr[i];
				carr[i]=val;
				i++;
			}
			System.out.println(Arrays.toString(carr));
			int start=-1,end=-1;
			System.out.println(x);
			for(i=0;i<=n;i++){
				for(int j=i;j<=n;j++)
					if(carr[j]-carr[i]==x){
						start=i;
						end=j;
						break;
					}
				if(start==-1||end==-1)
					continue;
				else
					break;
				
			}
			
			if(start==-1||end==-1)
				pw.println(-1);
			else
				pw.println((start+1)+" "+end);
		}
		pw.flush();
		pw.close();
	}
}
